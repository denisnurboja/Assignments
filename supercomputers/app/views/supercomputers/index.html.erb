<p id="notice"><%= notice %></p>
<h1>The Top Supercomputers:</h1>
<table class="table">
  <thead>
    <tr>
      <th>Rank</th>
      <th>Name</th>
      <th>City</th>
      <th>Country</th>
      <th>Speed (TFlop/s)</th>
      <th>Memory (GB)</th>
      <th>Power (kW)</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @supercomputers.each do |supercomputer| %>
    <tr>
      <td><%= supercomputer.rank %></td>
      <td><%= supercomputer.name %></td>
      <td><%= supercomputer.city %></td>
      <td><%= supercomputer.country %></td>
      <td><%= number_to_human(supercomputer.speed) %></td>
      <td><%= number_to_human(supercomputer.memory) %></td>
      <td><%= number_to_human(supercomputer.power) %></td>
      <td><%= link_to 'Show', supercomputer %></td>
      <td><%= link_to 'Edit', edit_supercomputer_path(supercomputer) %></td>
      <td><%= link_to 'Destroy', supercomputer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>
